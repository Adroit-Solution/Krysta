<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Test_Series</title>
        <link rel="stylesheet" href="~/lib/twitter-bootstrap/css/bootstrap.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Test_Series.styles.css" asp-append-version="true" />
    @await RenderSectionAsync("CssFile", required: false)
    <script src="https://code.iconify.design/2/2.1.0/iconify.min.js"></script>
</head>
<body >
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-black border-bottom box-shadow mb-5 stickytop">
            <div class="container-fluid bg-black">
                <a class="navbar-brand text-light" asp-area="" asp-controller="Home" asp-action="Index">Krsta</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1 bg-black">
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Plans">Plans</a>
                        </li><li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="About">About</a>
                        </li><li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Issue" asp-action="Index">Contact Us</a>
                        </li><li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Papers">My Papers</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="">
        <main role="main" class="pb-3 body">

            @RenderBody()
        </main>
    </div>

    <footer class="footer1 bg-black text-white">
        <div class="container">
            <div class=row>
                <div class=col-md-4>
                    <ul>
                        Get in Touch
                        <li>Call</li>
                        <li>Mail</li>
                        <li>Whatsapp</li>
                    </ul>
                </div>
                <div class=col-md-4>
                    <ul>
                        Quick Links
                        <li>Career</li>
                        <li>Think</li>
                    </ul>
                </div>
                <div class=col-md-4>
                    <ul>
                        Policies
                    </ul>
                </div>
            </div>
            &copy; @DateTime.Today - Test_Series - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
